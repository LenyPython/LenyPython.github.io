{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "concurrently 'yarn:watch:*'",
    "watch:storybook": "storybook dev -p 6006",
    "watch:tailwind": "tailwindcss -i ./assets/css/main.css -o ./dist/tailwind.css --watch",
    "build:tailwind": "tailwindcss -i ./assets/css/main.css -o ./dist/tailwind.css --watch",
    "build-storybook": "yarn build:tailwind && storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.0-alpha.0",
    "@storybook/addon-interactions": "^7.1.0-alpha.0",
    "@storybook/addon-links": "^7.1.0-alpha.0",
    "@storybook/blocks": "^7.1.0-alpha.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.1.0-alpha.0",
    "@storybook/vue3-vite": "^7.1.0-alpha.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "nuxt": "^3.3.2",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0-alpha.0",
    "tailwindcss": "^3.3.1",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1"
  },
  "peerDependencies": {}
